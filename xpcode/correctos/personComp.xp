program

type person{
  char[10] name;
  int id;
  float height;
  float weight;
}

function void printPerson(var person p){
  write("Person Data is : ");
  write(p.id," ", p.name, " ",p.height, " ", p.weight);
}

function void readPerson(var person p){
  read(p.id);
  read(p.height);
  read(p.weight);
  read(p.name);
}

function bool personComp(var person a, var person b){
  return a.id < b.id;
}

proc bool compare(person,person);

function bool anotherComp(var compare f, var person a, var person b){
  return f(a,b);
}


begin
  let person a;
  let person b;
  let person c;
  if(anotherComp(personComp,a,b)){
    if(anotherComp(personComp,a,c)){
      printPerson(a);
    } else {
      printPerson(c);
    };
  } else {
    if(anotherComp(personComp,b,c)){
      printPerson(b);
    } else {
      printPerson(c);
    };
  };

end
